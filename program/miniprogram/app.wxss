/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}

input{
  border-bottom: 1px solid #dcdcdc;
  text-align: center;
  color: #9e9e9e;
}

page {
  background: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
